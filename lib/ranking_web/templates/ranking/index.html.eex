<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Score</th>
      <th>Rank</th>
    </tr>
  </thead>
  <tbody>
<%= for ranking <- @rankings do %>
    <tr>
      <td><%= link ranking.name, to: Routes.ranking_path(@conn, :index) %></td>
      <td><%= ranking.score %></td>
      <td><%= ranking.rank %></td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "Add New Ranking!!", to: Routes.ranking_path(@conn, :new) %></span>
